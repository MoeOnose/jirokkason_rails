<% if article.favorited_by?(session[:session_id]) %>
	<%# binding.pry %>
	<%= link_to article_favorites_path(article_id: article.id), remote: true, method: :delete do %>
		<div class="favo_img share_archive" id="favorite-btn-<%= article.id %>"></div>
	<% end %>
<% else %>
	<%= link_to article_favorites_path(article_id: article.id), remote: true, method: :post do %>
		<div class="favo_img share_archive_fav" id="favorite-btn-<%= article.id %>"></div>
	<% end %>
<% end %>